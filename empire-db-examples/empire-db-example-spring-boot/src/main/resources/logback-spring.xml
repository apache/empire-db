<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="true">
    <appender name="default" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d{ISO8601} %5p] %40.40c:%4L [%-8t] - %m%n</pattern>
        </encoder>
    </appender>

    <root level="info">
        <appender-ref ref="default"/>
    </root>
</configuration>
